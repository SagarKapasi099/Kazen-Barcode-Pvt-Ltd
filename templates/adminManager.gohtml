    <body class="">
    <div class="wrapper" style="background-color:#f4f3ef">

        <div class="main-panel" style="width:100% !important">

            <nav class="navbar navbar-expand-lg navbar-absolute fixed-top bg-white" style="
    position: fixed;
">
                <div class="container-fluid">
                    <div class="navbar-wrapper">
                        <div class="navbar-toggle" style="display:none">
                            <button type="button" class="navbar-toggler">
                                <span class="navbar-toggler-bar bar1"></span>
                                <span class="navbar-toggler-bar bar2"></span>
                                <span class="navbar-toggler-bar bar3"></span>
                            </button>
                        </div>
                        <a class="navbar-brand" href="#pablo">Kazen Barcode Pvt Ltd - Admin Panel</a>
                    </div>


                </div>
            </nav>


            <div class="content" style="min-height:inherit;">

                <div class="row" style="padding-top:4em">
                    <div class="container">
                        <div class="card card-user">

                            <div class="card-body">
                                <div class="author">
                                    <a>
                                        <img class="avatar border-gray" src="/static/images/avatars/avatar.svg" alt="...">
                                        <h5 class="title"></h5>
                                    </a>

                                </div>
                                <form>
                                    <div class="col-md-6 mx-auto">
                                        <div class="form-group">
                                            <label>USERNAME</label>
                                            <input type="text" class="form-control" id="username" placeholder="username" value="">
                                        </div>
                                    </div>

                                    <div class="col-md-6 mx-auto">
                                        <div class="form-group">
                                            <label>PASSWORD</label>
                                            <input type="password" autocomplete="off" class="form-control" id="password" placeholder="password" value="">
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="update ml-auto mr-auto">
                                            <button id="signIn" class="btn btn-primary btn-round">Sign In</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="update ml-auto mr-auto">
                                            <button id="switchUser" style="display:none" class="btn btn-warning btn-round">Switch User
                                            </button>
                                        </div>
                                    </div>
                                </form>
                            </div>

                        </div>

                    </div>

                </div>

            </div>

        </div>
    </div>

